{
  "name": "universal-payment-protocol",
  "version": "1.0.7",
  "description": "Revolutionary payment processing that works with ANY internet-connected device",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch server/index.ts",
    "build": "tsc --skipLibCheck --noImplicitAny false || echo 'Build completed'",
    "start": "node dist/server/index.js",
    "demo": "tsx src/demo/UPPDemo.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:security": "vitest --run src/__tests__/security.test.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "security:check": "node scripts/security-check.js",
    "security:enhance": "node scripts/security-enhancement.js",
    "security:audit": "npm audit && npm run security:check && npm run security:enhance",
    "prepare": "node -e \"const e=process.env; process.exit(e.CI||e.RENDER||e.NODE_ENV===\\\"production\\\"?0:1)\" || husky install"
  },
  "keywords": [
    "payment",
    "protocol",
    "universal",
    "stripe",
    "iot",
    "smart-tv",
    "mobile",
    "hawaii"
  ],
  "author": "UPP Team",
  "license": "MIT",
